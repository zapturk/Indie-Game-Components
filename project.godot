; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Indie Game Components"
config/description="A collection of high-quality, reusable components for Godot 4.6."
config/features=PackedStringArray("4.6")
config/icon="res://icon.png"

[display]

window/size/viewport_width=512
window/size/viewport_height=288
window/size/window_width_override=1536
window/size/window_height_override=864
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/indie_game_components/plugin.cfg")

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/decals/filter=0
textures/light_projectors/filter=0
